<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="PcgsInvUi.Views.FindWindow"
        Title="FindWindow">
    <Grid ColumnDefinitions="Auto, *" RowDefinitions="*, *">
        <Label Content="PCGS #:" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="5"/>
        <TextBox Grid.Row="0" Grid.Column="1" Margin="5"/>
        <Button Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Content="Find"
                HorizontalAlignment="Center" Margin="5" Width="75"/>
    </Grid>
</Window>
